
/*
 * programa que muestra los nÃºmeros del 10 al 1
 */
public class Ejemplo2For {
    public static void main(String[] args) {
        int i;
	// este bucle comienza con el valor entero 10(variable i), hasta que i sea mayor que 0, y a i se le ira restando 1 en cada vuelta
        for(i=10; i>0;i--)
            System.out.println(i + " ");
    }
}

